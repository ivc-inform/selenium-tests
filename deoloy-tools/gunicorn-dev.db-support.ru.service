[Unit]
Description=Gunicorn server for SITENAME

[Service]
Restart=on-failure
User=uandrew
WorkingDirectory=/home/uandrew/nginx-hosting/sites/SITENAME/selenium-tests
ExecStart=/home/uandrew/.virtualenvs/selenium-tests/bin/gunicorn --bind unix:/tmp/SITENAME.socket project.wsgi:application

[Install]
WantedBy=multi-user.target