       listen 81;

       server_name dev.db-support.ru;

       location /static {
               alias /home/uandrew/nginx-hosting/sites/dev.db-support.ru/static;
       }

       #location / {
       #        proxy_pass http://localhost:8000;
       #}

       location / {
               proxy_set_header Host $host;
               proxy_pass http://unix:/tmp/dev.db-support.ru.socket;
       }
}
